<?xml version="1.0" encoding="utf-8"?>
<data>

    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="qwaco_contact.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div name="qwaco_setting_container" position="inside">
                <div class="col-lg-12 o_setting_box">
                    <div class="o_setting_left_pane"/>
                    <div class="o_setting_right_pane">
                        <div class="content-group">
                            <div class="row">
                                <label for="qwaco_sale_product_credit" string="Sale Product Credit" class="col-3 o_light_label"/>
                                <field name="qwaco_sale_product_credit" options="{'no_create': True, 'no_open': True}"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>

</data>
